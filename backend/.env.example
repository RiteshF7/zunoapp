# Zuno backend — copy to .env and fill values.
# Run ./scripts/resolve-env.sh to generate active .env for the selected mode.

ZUNO_MODE=development

# Supabase — Dev
SUPABASE_URL_DEV=https://your-dev-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY_DEV=
SUPABASE_JWT_SECRET_DEV=
SUPABASE_DB_PASSWORD_DEV=

# Supabase — Prod
SUPABASE_URL_PROD=https://your-prod-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY_PROD=
SUPABASE_JWT_SECRET_PROD=
SUPABASE_DB_PASSWORD_PROD=

# Vertex AI — Dev
GCP_PROJECT_ID_DEV=
GCP_CREDENTIALS_JSON_DEV=

# Vertex AI — Prod
GCP_PROJECT_ID_PROD=
GCP_CREDENTIALS_JSON_PROD=

# Backend — Dev
ENVIRONMENT_DEV=development
CORS_ORIGINS_DEV=http://localhost:5173,http://localhost:8081,http://localhost:19006,http://localhost:8000

# Backend — Prod (add your frontend host)
ENVIRONMENT_PROD=production
CORS_ORIGINS_PROD=https://zunoapp.onrender.com,https://app.zuno.com,http://localhost,capacitor://localhost

# Shared
GCP_LOCATION=us-central1
RAG_CHUNK_SIZE=500
RAG_CHUNK_OVERLAP=50
RAG_TOP_K=8
VERTEX_EMBEDDING_MODEL=text-embedding-005
VERTEX_LLM_MODEL=gemini-2.0-flash-001
BACKEND_PORT=8000
